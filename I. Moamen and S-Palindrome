//  IN THE NAME OF ALLAH...
#include <string>
#include <iostream>
#include <cmath>
#include <algorithm>

using namespace std;

int main()
{
    int x, o=0;
    char a[100000];
    cin >> x;
    for (int i=0; i<x; i++){
        a[i] = 'a';
        if (x%2!=0){
            a[(x-1)/2] = 'b';
        }
        else {
            a[(x/2)-1] = 'b';
            a[x/2] = 'b';
        }
        cout << a[i];
    }
}
