// IN THE NAME OF ALLAH ^_^
#include <iostream>
#include <string>
#include <cmath>
#include <algorithm>

using namespace std;

int main()
{
    string s;
    cin >> s;
    if (s.find("1111111")!=-1 || s.find("0000000")!=-1){
        cout << "YES" << endl;
    }
    else {
        cout << "NO" << endl;
    }
}
