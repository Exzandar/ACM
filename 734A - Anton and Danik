// IN THE NAME OF ALLAH ^_^
#include <iostream>
#include <string>
#include <cmath>
#include <algorithm>

using namespace std;

int main ()
{
    int n, A=0, D=0;
    cin >> n;
    string s;
    cin >> s;
    for (int i=0; i<n; i++){
        if (s[i]=='A'){
            A++;
        }
    }
    D = n-A;
    if (D==A){
        cout << "Friendship";
    }
    else if (A>D){
        cout << "Anton";
    }
    else {
        cout << "Danik";
    }
}
