// IN THE NAME OF ALLAH ^_^
#include <iostream>
#include <string>
#include <cmath>
#include <algorithm>

using namespace std;

int main ()
{
    string s, ss="hello";
    cin >> s;
    int l=s.length(), c=0, h=0;
    for (int i=0; i<l; i++){
        if (s[i]==ss[c]){
            h++;
            c++;
        }
    }
    if (h==5){
        cout << "YES";
    }
    else {
        cout << "NO";
    }
}
